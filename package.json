{
  "name": "agent-api-sdk",
  "version": "1.0.0",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": {
        "require": "./src/index.d.ts",
        "default": "./src/index.d.ts"
      },
      "default": {
        "require": "./dist/node/moobius-api-sdk.cjs",
        "default": "./dist/moobius-api-sdk.js"
      }
    }
  },
  "repository": "https://github.com/groupultra/agent-api-sdk.git",
  "author": "moobius",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c -m",
    "prettier:fix": "prettier src --check --write",
    "lint": "eslint .",
    "fix": "npm run prettier:fix && npm run lint",
    "start:client": "node ./sandbox/server.js",
    "start:node": "node ./sandbox/node-agent.js"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "prettier": "^3.2.5",
    "rollup": "^2.79.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.9.5"
  },
  "commitlint": {
    "rules": {
      "header-max-length": [
        2,
        "always",
        130
      ]
    },
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "axios": "^1.7.2",
    "form-data": "^4.0.0",
    "store2": "^2.14.3",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1",
    "ws": "^8.17.0"
  }
}
